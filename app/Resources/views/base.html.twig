<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Initiative Big Data Orange| Bienvenue!{% endblock %}</title>
        {% block stylesheets %}
            <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
            <!-- Bootstrap 3.3.5 -->
            <link rel="stylesheet" href="{{ asset('themes/bootstrap/css/bootstrap.min.css')}}">
            <!-- Font Awesome -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
            <!-- Ionicons -->
            <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
            <!-- jvectormap -->
            <link rel="stylesheet" href="{{ asset('themes/plugins/jvectormap/jquery-jvectormap-1.2.2.css')}}">
            <!-- Theme style -->
            <link rel="stylesheet" href="{{ asset('themes/dist/css/AdminLTE.min.css')}}">
            <!-- AdminLTE Skins. Choose a skin from the css/skins
                 folder instead of downloading all of them to reduce the load. -->
            <link rel="stylesheet" href="{{ asset('themes/dist/css/skins/_all-skins.min.css')}}">

            <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
            <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
            <!--[if lt IE 9]>
                <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
                <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
            <![endif]-->
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico.png') }}" />
    </head>
    <body class="hold-transition skin-adminlte-orange sidebar-mini">
    <div class="wrapper">

        <header class="main-header">

            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Orange</b></span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                  
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="{{ asset('themes/dist/img/user2-160x160.jpg') }}" class="user-image" alt="User Image">
                                <span class="hidden-xs">{{ app.user.username }}</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="{{ asset('themes/dist/img/user2-160x160.jpg') }}" class="img-circle" alt="User Image">
                                    <p>
                                        {{ app.user.username }} - Web Developer
                                        
                                    </p>
                                </li>
                              
                                    <div class="pull-right">
                                        <a href="{{ path('fos_user_security_logout') }}" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        
                </div>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="{{ asset('themes/dist/img/user2-160x160.jpg') }}" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>{{ app.user.username }}</p>       
                    </div>
                </div>
                <!-- search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    {% if is_granted('ROLE_SUPER_ADMIN') %}
                    <li class="header">Expérience Client</li>
                    <li class="active treeview">
                        <a href="http://localhost:8080/yenna/web/superadmin/index">
                            <i class="lte-icon-gauge"></i> <span>Géolocalisation</span>
                        </a>
                    </li>
                    {% endif %}
                    {% if is_granted('ROLE_SUPER_ADMIN') or is_granted('ROLE_ADMIN') %}
                    <!--li class="header">Cas d'utilisation 2</li>
                    <li class="treeview">
                        <a href="#">
                            <i class="lte-icon-e-dashboard"></i>
                            <span>Sous menu 1</span>
                            <i class="fa fa-caret-left pull-right"></i>
                        </a>
                        
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="lte-icon-laptop"></i>
                            <span>Sous menu 2</span>
                            <i class="fa fa-caret-left pull-right"></i>
                        </a>
                        
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="lte-icon-forms-edit"></i> <span>Sous menu 3</span>
                            <i class="fa fa-caret-left pull-right"></i>
                        </a>
                        
                    </li>
                        {% endif %}
                    {% if is_granted('ROLE_SUPER_ADMIN') or is_granted('ROLE_ADMIN') or is_granted('ROLE_GUEST') %}
                    <li class="header">Cas d'utilisation 3</li>
                    <li class="treeview">
                        <a href="#">
                            <i class="lte-icon-sous-menu"></i> <span>Sous menu 1 </span>
                            <i class="fa fa-caret-left pull-right"></i>
                        </a>
                        
                    </li>
                    <li>
                        <a href="pages/calendar.html">
                            <i class="lte-icon-calendar-month"></i> <span>Sous menu 2</span>
                            <small class="label pull-right bg-red"></small>
                        </a>
                    </li>
                         
                    <li><a href="documentation/index.html"><i class="lte-icon-book-download"></i> <span>Documentation</span></a></li>
                   

                    {% endif %}
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Géolocalisation des clients</h1>
                <ol class="breadcrumb">
                    <li><a href="#">Expérience Client</a></li>
                    <li class="active">Orange Money</li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
        {% block body %}
        
        {% endblock body %}
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
              
            </div>
            <strong>Copyright 2016 Orange</a>.</strong> All rights reserved.
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="lte-icon-settings"></i></a></li>
            </ul>
            <!-- Tab panes -->
           

    </div><!-- ./wrapper -->
        {% block javascripts %}

            <!-- jQuery 2.1.4 -->
            <script src="{{ asset('themes/plugins/jQuery/jQuery-2.1.4.min.js') }}"></script>
            <!-- Bootstrap 3.3.5 -->
            <script src="{{ asset('themes/bootstrap/js/bootstrap.min.js') }}"></script>
            <!-- FastClick -->
            <script src="{{ asset('themes/plugins/fastclick/fastclick.min.js') }}"></script>
            <!-- AdminLTE App -->
            <script src="{{ asset('themes/dist/js/app.min.js') }}"></script>
            <!-- jQuery Knob -->
            <script src="{{ asset('themes/../../plugins/knob/jquery.knob.js') }}"></script>
            <!-- Sparkline -->
            <script src="{{ asset('themes/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
            <!-- jvectormap -->
            <script src="{{ asset('themes/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
            <script src="{{ asset('themes/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
            <!-- SlimScroll 1.3.0 -->
            <script src="{{ asset('themes/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
            <!-- ChartJS 1.0.1 -->
            <script src="{{ asset('themes/plugins/chartjs/Chart.min.js') }}"></script>
            <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
            <script src="{{ asset('themes/dist/js/pages/dashboard2.js') }}"></script>
            <!-- AdminLTE for demo purposes -->
            <script src="{{ asset('themes/dist/js/demo.js') }}"></script>

            <script>
                $(function () {
                    /* jQueryKnob */
                    $(".knob").knob();
                });
            </script>
          
        {% endblock %}
    </body>
</html>
